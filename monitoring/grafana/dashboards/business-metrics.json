          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawSql": "WITH funnel_data AS (\n  SELECT \n    COUNT(DISTINCT CASE WHEN event_type IN ('user_login', 'view_tariffs', 'tariff_switch', 'energy_consumed', 'incentive_claim', 'bill_payment') THEN customer_id END) as active_users,\n    COUNT(DISTINCT CASE WHEN event_type = 'view_tariffs' THEN customer_id END) as tariff_viewers,\n    COUNT(DISTINCT CASE WHEN event_type = 'tariff_switch' THEN customer_id END) as converters,\n    COUNT(DISTINCT CASE WHEN event_type = 'bill_payment' THEN customer_id END) as payers\n  FROM events \n  WHERE event_time >= $__timeFrom() AND event_time <= $__timeTo()\n)\nSELECT \n  active_users as \"Active Users\",\n  tariff_viewers as \"Tariff Views\",\n  converters as \"Conversions\",\n  payers as \"Payments\",\n  ROUND((tariff_viewers::NUMERIC / NULLIF(active_users, 0) * 100), 1) as \"View Rate %\",\n  ROUND((converters::NUMERIC / NULLIF(tariff_viewers, 0) * 100), 1) as \"Conversion Rate %\",\n  ROUND((payers::NUMERIC / NULLIF(converters, 0) * 100), 1) as \"Payment Rate %\"\nFROM funnel_data",
          "refId": "A"
        }
      ],
      "title": "Customer Engagement Funnel",
      "type": "stat"
