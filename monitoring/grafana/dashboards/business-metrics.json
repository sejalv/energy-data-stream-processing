          "editorMode": "code",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH current_metrics AS (\n    SELECT \n        SUM(tariff_switches) as total_switches,\n        SUM(total_energy_consumed) as total_consumption,\n        AVG(active_customers) as avg_customers,\n        SUM(total_payments) as total_revenue,\n        SUM(incentive_claims) as total_incentives,\n        AVG(avg_payment_amount) as avg_payment\n    FROM hourly_business_metrics \n    WHERE hour >= $__timeFrom() AND hour <= $__timeTo()\n), \nprevious_metrics AS (\n    SELECT \n        SUM(tariff_switches) as prev_switches,\n        SUM(total_energy_consumed) as prev_consumption,\n        SUM(total_payments) as prev_revenue\n    FROM hourly_business_metrics \n    WHERE hour >= ($__timeFrom()::timestamp - ($__timeTo()::timestamp - $__timeFrom()::timestamp))\n      AND hour < $__timeFrom()\n)\nSELECT \n    cm.total_switches as \"Total Switches\",\n    ROUND(cm.total_consumption, 2) as \"Total Consumption (kWh)\",\n    ROUND(cm.avg_customers, 0) as \"Avg Active Customers\",\n    ROUND(cm.total_revenue, 2) as \"Total Revenue (€)\",\n    cm.total_incentives as \"Incentive Claims\",\n    ROUND(cm.avg_payment, 2) as \"Avg Payment (€)\",\n    CASE \n        WHEN pm.prev_switches > 0 THEN \n            ROUND(((cm.total_switches - pm.prev_switches)::NUMERIC / pm.prev_switches * 100), 1)\n        ELSE 0 \n    END as \"Switch Growth %\",\n    CASE \n        WHEN pm.prev_revenue > 0 THEN \n            ROUND(((cm.total_revenue - pm.prev_revenue)::NUMERIC / pm.prev_revenue * 100), 1)\n        ELSE 0 \n    END as \"Revenue Growth %\"\nFROM current_metrics cm, previous_metrics pm;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
