          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH customer_segments AS (\n    SELECT \n        hour,\n        -- Segment customers by behavior\n        CASE \n            WHEN green_tariff_switches > 0 THEN 'Green Adopters'\n            WHEN tariff_switches > 0 THEN 'Price Switchers'\n            ELSE 'Stable Customers'\n        END as customer_segment,\n        \n        total_energy_consumed,\n        tariff_switches,\n        total_switch_revenue,\n        active_customers,\n        \n        -- Calculate segment performance\n        ROUND(total_energy_consumed / NULLIF(active_customers, 0), 2) as avg_consumption_per_customer,\n        ROUND(total_switch_revenue / NULLIF(active_customers, 0), 2) as avg_revenue_per_customer\n        \n    FROM hourly_business_metrics\n    WHERE $__timeFilter(hour)\n)\nSELECT \n    hour as time,\n    customer_segment as \"Customer Segment\",\n    \n    -- Segment metrics\n    total_energy_consumed as \"Energy Demand\",\n    active_customers as \"Active Customers\",\n    -- tariff_switches as \"Switches\",\n    \n    -- Performance indicators\n    avg_consumption_per_customer as \"Avg Consumption per Customer\",\n    avg_revenue_per_customer as \"Avg Revenue per Customer\",\n    \n    -- Conversion rates\n    CASE \n        WHEN active_customers > 0 THEN \n            ROUND((tariff_switches::DECIMAL / active_customers) * 100, 2)\n        ELSE 0 \n    END as \"Switch Conversion Rate %\",\n    \n    -- Segment health score\n    CASE \n        WHEN avg_revenue_per_customer > 50 AND avg_consumption_per_customer > 20 THEN 'High Value'\n        WHEN avg_revenue_per_customer > 25 OR avg_consumption_per_customer > 15 THEN 'Medium Value'\n        ELSE 'Low Value'\n    END as segment_value_tier\n\nFROM customer_segments\nORDER BY hour, customer_segment;",
